{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "dist/",
        "node_modules",
        "tests/"
      ],
      "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"]
    }
}
